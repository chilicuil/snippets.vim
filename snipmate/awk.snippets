snippet #!
	#!/bin/awk -f
	
snippet if
	if (${1:condition}) ${2:#statements}
snippet for
	for (${1:condition}) {
		${2:#statements}
	}
snippet fori
	${2:i}=0; while ($2 < "${1:count}") {
		$2=$2 + 1;
		${3:#statements}
	}
snippet while
	while (${1:condition}) {
		${2:#statements}
	}
snippet whilei
	${2:i}=0; while ($2 < "${1:count}") {
		$2=$2 + 1;
		${3:#statements}
	}
snippet fun
	function _${1:function}(${2:parameters}) {
	    ${3:#statements}
	}
